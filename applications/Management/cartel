const { cartel } = require('../../base/Clients/Global.Clients');
const { Mongoose } = require('../../database/Global.MongoDB.Driver');
const client = global.client = new cartel();
const { GUILD } = require('../../base/Reference/Settings');
const { Collection } = require('discord.js');

// Client Ayarları (Başlangıç)
client.botİsmi = "Management"
client.invites = new Collection();
// Client Ayarları (SON)

Mongoose.Connect()
GUILD.fetch(sistem.SUNUCU.GUILD)
client.fetchCommands(true, false)
client.fetchEvents()
client.connect(sistem.TOKENLER.Management)

